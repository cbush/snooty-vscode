{
	"name": "snooty",
	"description": "Snooty is a next-generation documentation authoring IDE.",
	"author": "MongoDB",
	"license": "SEE LICENSE IN LICENSE",
	"version": "0.2.1",
	"icon": "misc/icon.png",
	"galleryBanner": {
		"color": "#FFF",
		"theme": "light"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mongodb/snooty-vscode"
	},
	"publisher": "snooty",
	"categories": [
		"Programming Languages",
		"Linters"
	],
	"keywords": [],
	"engines": {
		"vscode": "^1.45.1"
	},
	"activationEvents": [
		"workspaceContains:snooty.toml"
	],
	"main": "./out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Snooty",
			"properties": {
				"snooty.conflictingExtensions": {
					"type": "array",
					"default": [
						"tht13.rst-vscode"
					],
					"description": "Extensions that might lead to conflicts.",
					"scope": "application"
				},
				"snooty.languageServerPath": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"description": "Override the path to Snooty. Only required for Snooty developers."
				},
				"snooty.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VS Code and the language server."
				},
				"snooty.spigot.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enables the Snooty Spigot extension for additional ref-role support (experimental)"
				},
				"snooty.spigot.sourceRelativePath": {
					"type": "string",
					"default": "source/",
					"description": "If snooty.spigot.enabled, specifies the source directory to search for ref-roles relative to the Workspace root."
				}
			}
		},
		"languages": [
			{
				"id": "restructuredtext",
				"aliases": [
					"reStructuredText",
					"restructuredtext",
					"ReStructured Text",
					"reStructured Text",
					"RST",
					"ReST",
					"reST"
				],
				"extensions": [
					".rst",
					".rest",
					".txt"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "restructuredtext",
				"scopeName": "text.restructuredtext",
				"path": "./syntaxes/restructuredtext.tmLanguage"
			}
		],
		"commands": [
			{
				"command": "snooty.restart",
				"title": "Restart Snooty",
				"description": "Sometimes, it's just best to try turning it off and on again"
			}
		],
		"snippets": [
			{
				"language": "restructuredtext",
				"path": "./snippets/snooty.code-snippets"
			}
		]
	},
	"extensionDependencies": [
		"bungcip.better-toml"
	],
	"runtimeDependencies": [
		{
			"description": "Snooty Language Server for macOS (x64)",
			"platforms": {
				"x86_64 darwin": "https://github.com/mongodb/snooty-parser/releases/download/v0.5.6/snooty-v0.5.6-darwin_x86_64.zip",
				"x86_64 linux": "https://github.com/mongodb/snooty-parser/releases/download/v0.5.6/snooty-v0.5.6-linux_x86_64.zip"
			},
			"installPath": ".snooty",
			"installTestPath": ".snooty/snooty/snooty"
		}
	],
	"scripts": {
		"vscode:prepublish": "export NODE_ENV=production && make clean && make snippets && npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"test": "jest",
		"coverage": "jest --coverage"
	},
	"dependencies": {
		"@types/deep-equal": "^1.0.1",
		"@types/mime": "^2.0.1",
		"@types/yauzl-promise": "^2.1.0",
		"deep-equal": "^2.0.3",
		"globby": "^11.0.1",
		"https-proxy-agent": "^2.2.4",
		"mime": "^2.4.4",
		"mkdirp": "^0.5.1",
		"open": "^6.4.0",
		"tmp-promise": "^2.0.2",
		"vscode-languageclient": "^5.2.1",
		"vscode-languageserver": "^6.1.1",
		"vscode-languageserver-textdocument": "^1.0.1",
		"yauzl": "^2.10.0",
		"yauzl-promise": "^2.1.3"
	},
	"devDependencies": {
		"@types/jest": "^26.0.13",
		"@types/mkdirp": "^1.0.0",
		"@types/node": "^8.10.58",
		"@types/vscode": "1.45.1",
		"@types/yauzl": "^2.9.1",
		"jest": "^26.4.2",
		"mocha": "^8.1.3",
		"ts-jest": "^26.3.0",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"__metadata": {
		"id": "dc396dd1-bb7b-4657-9e79-041354bf3f94",
		"publisherDisplayName": "snooty",
		"publisherId": "4efa83cd-c905-4f5c-8326-f5d19354c314"
	}
}